{
  "jupyter-lite-schema-version": 0,
  "jupyter-config-data": {
    "disabledExtensions": [
      "@jupyterlab/drawio-extension",
      "jupyterlab-kernel-spy",
      "jupyterlab-tour"
    ],
    "litePluginSettings": {
      "@jupyterlite/lab-extension:main": {
        "workspace": {
          "files": ["default_notebook.ipynb"]
        },
        "toolbar": {
          "items": [
            {
              "name": "Run",
              "command": "run-cell",
              "icon": "fa fa-play",
              "tooltip": "Run the current cell"
            },
            {
              "name": "Insert",
              "command": "insert-cell",
              "icon": "fa fa-plus",
              "tooltip": "Insert a new cell"
            },
            {
              "name": "download",
              "label": "Download",
              "args": {},
              "command": "docmanager:download",
              "icon": "ui-components:download",
              "rank": 50
            }
          ]
        }
      }
    }
  }
}
  