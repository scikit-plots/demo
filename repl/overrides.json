{
  "jupyter-lite-schema-version": 0,
  "jupyter-config-data": {
    "defaultKernelName": "python",
    "piplite_wheels": [
      "pip",
      "scikit-plots",
      "pandas"
    ],
    "litePluginSettings": {
      "@jupyterlite/pyolite-kernel:kernel": {
        "startupCode": [
          "import numpy as np",
          "import micropip",
          "await micropip.install('matplotlib')"
        ]
      },
      "@jupyterlab/notebook-extension:panel": {
        "toolbar": [
          {
            "name": "download",
            "label": "Download",
            "args": {},
            "command": "docmanager:download",
            "icon": "ui-components:download",
            "rank": 50
          }
        ]
      }
    }
  }
}