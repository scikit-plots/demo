{
  "jupyter-lite-schema-version": 0,
  "LiteBuildConfig": {
      "apps": ["lab", "retro"],
      "no_unused_shared_packages": true,
      "no_sourcemaps": true,
      "content": ["notebooks"],
      "contents": ["content"],
      "output_dir": "dist"
  },
  "jupyter-config-data": {
    "appName": "scikit-plots REPLite",
    "appUrl": "./repl",
    "piplite_wheels": [
      "pip",
      "scikit-plot",
      "pandas"
    ],
    "toolbar": 1,
    "defaultKernelName": "python",
    "@jupyterlite/pyolite-kernel:kernel": {
      "startupCode": [
        "import numpy as np",
        "import micropip",
        "await micropip.install('matplotlib')"
      ]
    },
    "disabledExtensions": [
      "@jupyterlite/javascript-kernel-extension",
      "@jupyter-widgets/jupyterlab-manager",
      "@jupyterlab/drawio-extension",
      "jupyterlab-kernel-spy",
      "jupyterlab-tour"
    ],
    "litePluginSettings": {
      "@jupyterlite/pyodide-kernel-extension:kernel": {
        "pyodideUrl": "https://cdn.jsdelivr.net/pyodide/v0.26.0/full/pyodide.js",
        "kernelName": "pyodide-v0.26"
      }
    }
  },
  "kernelspecs": {
    "pyodide-v0.26": {
      "name": "python3",
      "display_name": "Python3 (Pyodide)"
    }
  }
}